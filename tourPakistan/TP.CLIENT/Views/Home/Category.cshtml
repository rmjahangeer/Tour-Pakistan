
@{
    ViewBag.Title = "Category";
}


<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; minimum-scale=1.0; user-scalable=0;" />
    <meta name="author" content="Travelogue" />

    <title>Travelogue | Category Page</title>
    <link rel="shortcut icon" href="~/Images/img/favicon.ico">

    <!-- ### CSS Stylesheets ##################################################################### -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/normalize.css" /><!-- CSS: Normalize -->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome/css/font-awesome.min.css" /><!-- CSS: Font Awesome -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/styles.css" /><!-- CSS: Main CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/responsive.css" /><!-- CSS: Responsive CSS -->
</head>
<body class="index-category">

    <div id="container" class="container intro-effect-sidefixed">

        <a class="cd-primary-nav-trigger" id="trigger-menu" href="#0">
            <span class="cd-menu-icon"></span>
        </a>

        <!-- BOF: Fancy Search Form (content imported via jQuery: from search-form.html) -->
        <div id="travelogue-search" class="travelogue-search">
            <form action="search.html">
                <input class="travelogue-search-input" placeholder="Enter your search term..." type="text" value="" name="search" id="search">
                <input class="travelogue-search-submit" type="submit" value="">
                <span class="travelogue-icon-search fa fa-search"></span>
            </form>
        </div>
        <!-- EOF: Fancy Search Form (content imported via jQuery: from search-form.html) -->


        <div class="title hidden" id="title">
            <h1>Category: Paris</h1>
            <p class="subline">Keep calm and go to Paris</p>
        </div>

        <!-- Top Navigation -->
        <header class="header">
            <div class="bg-img">
                <img class="async-image hide" src="" data-src="/Images/img/category_paris.jpg" alt="" />
                <div class="overlay hidden" id="overlay"></div>
            </div>
        </header>
        <button class="trigger"><span>Trigger</span></button>
        <article class="content no-padding">
            <div class="title hidden" id="title2">
                <h1></h1>
                <p class="subline"></p>
            </div>
            <div class="no-margin">
                <div class="grid">
                    <figure class="single-item-effect big">
                        <img src="~/Images/img/gallery/paris-1.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>Bad day in <span>Paris</span></h2>
                                <p>Still better than a good day anywhere else</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                    <figure class="single-item-effect big">
                        <img src="~/Images/img/gallery/paris-2.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>Never too <span>old</span></h2>
                                <p>Let’s go fall in love</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                    <figure class="single-item-effect full">
                        <img src="~/Images/img/gallery/paris-13.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>Bon <span>appetite</span></h2>
                                <p>It is food for your soul</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                    <figure class="single-item-effect big">
                        <img src="~/Images/img/gallery/paris-9.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>I had it <span>all</span></h2>
                                <p>But I was waiting for you</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                    <figure class="single-item-effect big">
                        <img src="~/Images/img/gallery/22.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>Like the <span>sunrise</span></h2>
                                <p>Some things just have to happen</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                    <figure class="single-item-effect full">
                        <img src="~/Images/img/gallery/paris-3.jpg" alt="img01" />
                        <figcaption>
                            <div class="figcaption-border">
                                <h2>Live your own <span>story</span></h2>
                                <p>Don’t re-write someone elses</p>
                                <a href="single.html">View more</a>
                                <div class="figure-overlay"></div>
                            </div>
                        </figcaption>
                    </figure>
                </div>
            </div>
        </article>
    </div><!-- /container -->
    <!-- ### Sidebar Menu ##################################################################### -->
    <div id="st-container" class="st-container">
        @Html.Partial("_MenuPartial")
    </div>

    <!-- ### JS Scripts ##################################################################### -->
    <script src="~/Scripts/js/modernizr.custom.js"></script>
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/sidebarEffects.js"></script>
    <script src="~/Scripts/js/classie.js"></script>
    <script src="~/Scripts/js/custom.js"></script>
    <script src="~/Scripts/js/transition.js"></script>
    <script src="~/Scripts/js/jquery.ketchup.all.min.js"></script>
    <!--[if IE]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <a href="#0" class="back-to-top">Top</a>
    <!-- Google Code for Remarketing Tag -->

    <script type="text/javascript">
/* <![CDATA[ */
var google_conversion_id = 1008623479;
var google_custom_params = window.google_tag_params;
var google_remarketing_only = true;
/* ]]> */
    </script>
    <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js">
    </script>
    <noscript>
        <div style="display:inline;">
            <img height="1" width="1" style="border-style:none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/1008623479/?value=0&amp;guid=ON&amp;script=0" />
        </div>
    </noscript>
</body>
</html>
